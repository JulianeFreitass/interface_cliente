<%# A linha abaixo carrega no template o conteÃºdo total do arquivo header.ejs%>
<%-include('base_header') -%>

<!--==================== PORTFOLIO ====================-->
        <section class="portfolio section" id="portfolio">
          <h2 class="section__title">Projects</h2>
          <span class="section__subtitle">Most recent work</span>
          <div class="portfolio__container container">
              <div class="swiper-wrapper">
                  <!--==================== PORTFOLIO 1 ====================-->
                  <div class="portfolio__content grid swiper-slide">
                      <% for(var i=0; i< lista_projetos.length; ++i) { %>  
                      <img src=<%=lista_projetos[i].img %> alt="" class="portfolio__img">
                      <div class="portfolio__data">
                          <h3 class="portfolio__title"><%=lista_projetos[i].projeto %></h3>
                          <p class="portfolio__description">Inicio: <%=lista_projetos[i].inicio %></p>
                          <p class="portfolio__description">Fim: <%=lista_projetos[i].fim %></p>
                          <p class="portfolio__description"><%=lista_projetos[i].description %></p>
                          <a href="<%=lista_projetos[i].link %>" class="button button--flex button--smalll portfolio__button">
                              Demo
                              <i class="uil uil-arrow-right button__icon"></i>
                          </a>
                          
                      </div>
                      <% } %>
                  </div>
              
          </div>
      </section>

<%- include('base_footer') -%>